    "def findMean(x):\n",
    "    \n",
    "    y = 0\n",
    "    tot = 0\n",
    "    \n",
    "    for i in x:\n",
    "        y = y + i\n",
    "        tot = tot + 1\n",
    "    \n",
    "    mean = y / tot\n",
    "    \n",
    "    return mean, tot"
   ]
  }
